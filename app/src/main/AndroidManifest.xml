<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mohammedalani.go_find_me">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScanningActivity" />
        <activity android:name=".BeaconsFoundActivity" />
        <activity android:name=".BeaconDetailsActivity" />
        <activity android:name=".EnterLostBeaconInfoActivity" />
        <activity android:name=".FindBeaconValidationActivity" />
        <activity android:name=".ContactInfoActivity"></activity>
    </application>

</manifest>